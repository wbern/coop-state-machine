{
    "name": "coop-state-machine-frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "webpack-dev-server --mode development --env.WEBSOCKET_PORT=8080",
        "build": "webpack --mode production",
        "build-watch": "webpack --mode development --watch",
        "test": "jest"
    },
    "devDependencies": {
        "@babel/core": "^7.6.2",
        "ace-builds": "^1.4.6",
        "ajv": "^7.0.2",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "babel-preset-vue": "^2.0.2",
        "core-js": "^3.8.1",
        "css-loader": "*",
        "diff-match-patch": "^1.0.4",
        "eslint": "^7.16.0",
        "eslint-plugin-vue": "^7.3.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^4.5.0",
        "jest": "^26.6.3",
        "jest-serializer-vue": "^2.0.2",
        "json5-loader": "^4.0.1",
        "keen-ui": "^1.2.0",
        "moment": "^2.24.0",
        "popular-movie-quotes": "^1.2.0",
        "prettier": "^2.2.1",
        "raw-loader": "^4.0.2",
        "rxjs": "^6.5.3",
        "socket.io-client": "^3.0.4",
        "typescript": "*",
        "vue": "^2.6.10",
        "vue-eslint-parser": "^7.3.0",
        "vue-jest": "^3.0.5",
        "vue-loader": "^15.7.0",
        "style-loader": "^2.0.0",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.10",
        "vuex": "^3.1.1",
        "vuex-undo-redo": "^1.1.4",
        "webpack": "4.x.x",
        "webpack-cli": "3.x.x",
        "webpack-dev-server": "3.x.x"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "no-console": "off",
            "no-unused-vars": "off",
            "no-case-declarations": "off",
            "no-extra-boolean-cast": "off"
        },
        "parserOptions": {
            "parser": "babel-eslint",
            "ecmaVersion": 2018,
            "sourceType": "module"
        },
        "overrides": [
            {
                "files": [
                    "**/__tests__/*.{j,t}s?(x)"
                ],
                "env": {
                    "jest": true
                }
            }
        ]
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "dependencies": {
        "material-design-icons": "^3.0.1"
    }
}
