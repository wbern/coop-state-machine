{
    "name": "coop-state-machine-frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "webpack-dev-server --mode development src/main.js",
        "build": "webpack-cli --mode development src/main.js",
        "test": "jest"
    },
    "devDependencies": {
        "@babel/core": "^7.6.2",
        "ace-builds": "^1.4.6",
        "ajv": "^6.10.2",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "babel-preset-vue": "^2.0.2",
        "core-js": "^2.6.5",
        "css-loader": "*",
        "diff-match-patch": "^1.0.4",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "jest": "^24.9.0",
        "jest-serializer-vue": "^0.2.0",
        "keen-ui": "^1.2.0",
        "prettier": "^1.18.2",
        "raw-loader": "^3.1.0",
        "file-loader": "^4.2.0",
        "typescript": "*",
        "vue": "^2.6.10",
        "vue-jest": "^3.0.5",
        "vue-loader": "^15.7.0",
        "vue-template-compiler": "^2.6.10",
        "vuex": "^3.1.1",
        "vue-style-loader": "^4.1.2",
        "vuex-undo-redo": "^1.1.4",
        "webpack": ">=2",
        "webpack-cli": ">=2",
        "html-webpack-plugin": "^3.2.0",
        "webpack-dev-server": "^3.8.2"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "no-console": "off",
            "no-unused-vars": "off",
            "no-case-declarations": "off"
        },
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "overrides": [
            {
                "files": [
                    "**/__tests__/*.{j,t}s?(x)"
                ],
                "env": {
                    "jest": true
                }
            }
        ]
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "dependencies": {
        "material-design-icons": "^3.0.1"
    }
}
