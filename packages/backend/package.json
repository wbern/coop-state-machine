{
    "name": "coop-state-machine-backend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "",
        "start": "node index.js"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "parserOptions": {
            "parser": "babel-eslint",
            "ecmaVersion": 6
        },
        "extends": [
            "eslint:recommended"
        ],
        "rules": {
            "no-console": "off",
            "no-unused-vars": "off",
            "no-case-declarations": "off"
        },
        "overrides": [
            {
                "files": [
                    "**/__tests__/*.{j,t}s?(x)"
                ],
                "env": {
                    "jest": true
                }
            }
        ]
    },
    "dependencies": {
        "babel-eslint": "^10.0.1",
        "bluebird": "^3.7.1",
        "express": "^4.17.1",
        "socket.io": "^2.3.0",
        "sillyname": "^0.1.0"
    }
}
